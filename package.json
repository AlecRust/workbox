{
  "name": "@alecrust/workbox",
  "version": "0.1.0",
  "description": "Bun-first CLI for managing Git worktree sandboxes.",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlecRust/workbox.git"
  },
  "homepage": "https://github.com/AlecRust/workbox#readme",
  "bugs": {
    "url": "https://github.com/AlecRust/workbox/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "bin": {
    "workbox": "./src/cli.ts",
    "wkb": "./src/cli.ts"
  },
  "engines": {
    "bun": "1.3.6"
  },
  "scripts": {
    "prepare": "bun scripts/prepare.ts",
    "test": "bun test",
    "lint": "biome lint .",
    "check:types": "tsc --noEmit",
    "check:unused": "knip",
    "check": "bun run lint && bun run check:types && bun run check:unused",
    "format": "biome check --write ."
  },
  "dependencies": {
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@tsconfig/bun": "^1.0.10",
    "@types/bun": "^1.3.6",
    "knip": "^5.82.0",
    "lefthook": "^2.0.15",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "git",
    "worktree",
    "cli",
    "bun"
  ]
}
